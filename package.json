{
  "name": "visang-design-system",
  "description": "Visang Storybook design system",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/chromaui/learnstorybook-design-system.git",
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "lodash": "^4.17.15",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-draggable": "^4.1.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.1",
    "util": "^0.12.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build": "cross-env BABEL_ENV=production babel src -d dist"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/preset-create-react-app": "^1.3.1",
    "@storybook/react": "^5.2.5",
    "@types/jest": "^24.0.23",
    "auto": "^7.16.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.5.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  }
}
